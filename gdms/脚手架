--https://webpack.js.org/concepts/
--全部使用本地安装，保证上传版本服务器后，其他用户可以直接使用
--安装webpack
sudo cnpm install --save   webpack
--安装vue
sudo cnpm install --save vue-cli
--创建项目
sudo vue init webpack my-project
cd gdmsProject/
--初始化项目
cnpm install
--运行dev
cnpm run dev

--安装iview前端组件
cnpm install iview --save



